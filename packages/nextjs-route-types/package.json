{
  "name": "nextjs-route-types",
  "version": "2.0.0",
  "description": "Automatic type generation for Next.js app router routes",
  "bugs": "https://github.com/joulev/nextjs-route-types/issues",
  "repository": "joulev/nextjs-route-types",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "pnpm build --watch",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "dependencies": {
    "chokidar": "^3.5.3"
  },
  "devDependencies": {
    "@types/node": "20.14.2",
    "eslint": "8.57.0",
    "eslint-config-custom": "workspace:*",
    "next": "15.0.0",
    "tsconfig": "workspace:*",
    "tsup": "8.1.0",
    "typescript": "5.4.5",
    "webpack": "5.92.0"
  },
  "peerDependencies": {
    "next": "^15.0.0"
  }
}
